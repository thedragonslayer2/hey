-- anti afk and more
repeat wait() until game:IsLoaded()
local players = game:GetService"Players"
local client = players.LocalPlayer
if hookfunction and getrawmetatable and setreadonly then
	setreadonly(getrawmetatable(game), false)
	-- anti kick
	local oldkick
	oldkick = hookfunction(getrawmetatable(game).__namecall, function(Self, ...)
		if not checkcaller() then
			local method = tostring(getnamecallmethod())
			if method == "Kick" or method == "kick" then
				warn"Game Tried To Kick You!"
				return wait(math.huge)
			end
		end
		return oldkick(Self, ...)
	end)
end
-- anti afk
if getconnections then
	for i, v in next, getconnections(client.Idled) do v:Disable() end
else
	local vu = game:GetService"VirtualUser"
	client.Idled:Connect(function()
		pcall(function()
			vu:CaptureController()
			wait(1)
			vu:ClickButton2(Vector2.new())
		end)
		pcall(function()
			vu:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
			wait(1)
			vu:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
		end)
	end)
end
-- invisicam
spawn(function()
	while wait() do
		client.DevCameraOcclusionMode = "Invisicam"
		client.CameraMaxZoomDistance = 9e9
	end
end)
return warn"ANTI AFK ACTIVATED!"
