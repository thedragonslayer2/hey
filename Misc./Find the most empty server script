if not IrisNotificationMrJack then loadstring(game:HttpGet"https://raw.githubusercontent.com/thedragonslayer2/hey/main/Misc./iris%20notification%20function")()elseif IrisNotificationUserMrJack then IrisNotificationUserMrJack.ClearAllNotifications()end;if not getgenv().FailedServerID then getgenv().FailedServerID={}end;if not ThePlaceID or ThePlaceID==0 then ThePlaceID=game.PlaceId end;IrisNotificationMrJack(1,"Notification","Checking Servers!",9e9)local a,b,c,d=math.huge,"https://games.roblox.com/v1/games/"..ThePlaceID.."/servers/Public?sortOrder=Asc&limit=100"while task.wait()do local e=game:GetService"HttpService":JSONDecode(game:HttpGetAsync(b))local f=e.nextPageCursor;for g,h in next,e.data do if type(h)=="table"and h.id and tonumber(h.playing)and not table.find(getgenv().FailedServerID,h.id)then if h.id==game.JobId then d=h end;if a>h.playing or c and a==h.playing and tonumber(h.ping)and tonumber(c.ping)and h.ping<c.ping then a=h.playing;c=h end end end;if f then local i=b:find"&cursor="if i then b=b:gsub(b:sub(i),"")end;b=b.."&cursor="..f else break end end;wait(0.5)IrisNotificationUserMrJack.ClearAllNotifications()wait(0.5)if c.id==game.JobId or a==#game:GetService"Players":GetChildren()-1 and tonumber(d.ping)and tonumber(c.ping)and d.ping<c.ping then warn"Your current server is the most empty server atm"return IrisNotificationMrJack(1,"Notification","Your current server is the most empty server atm",2)else IrisNotificationMrJack(1,"Notification","Teleporting To Server\n"..c.id,2)table.insert(getgenv().FailedServerID,c.id)warn("Server Found! Players:",c.playing,"  PING:",c.ping)game:GetService"TeleportService":TeleportToPlaceInstance(ThePlaceID,c.id)end
