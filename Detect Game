spawn(function()
    if not game:IsLoaded() then repeat wait() until game:IsLoaded() end
    local vu = game:GetService("VirtualUser")
    game:GetService 'Players'.LocalPlayer.Idled:connect(function()
        pcall(function()
            vu:CaptureController()
            vu:ClickButton2(Vector2.new())
        end)
        pcall(function()
            vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            u:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        end)
    end)
end)
if game.PlaceId == 5765666934 or game.PlaceId == 5343994291 or game.PlaceId == 5765666554 then--outlaster
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Outlaster'))()
elseif game.PlaceId == 6610021055 then--Noob Army Tycoon
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Noob%20Army%20Tycoon'))()
elseif game.PlaceId == 6490016198 then--Slayer Tycoon
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Slayer%20Tycoon'))()
elseif game.PlaceId == 221718525 then--Ninja Tycoon
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Ninja%20Tycoon'))()
elseif game.PlaceId == 5534174456 then--Resort Tycoon üå¥
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Resort%20Tycoon'))()
elseif game.PlaceId == 6769764667 then--Factory Simulator
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Factory%20Simulator'))()
elseif game.PlaceId == 5233782396 then--Creatures of Sonaria
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Creatures%20of%20Sonaria'))()
elseif game.PlaceId == 4505214429 then--Soul Eater
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Soul%20Eater'))()
elseif game.PlaceId == 6704182071 then--Cooking Simulator
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Cooking%20Simulator'))()
elseif game.PlaceId == 6514274883 then--Dream Island ‚úàÔ∏è Tycoon
	loadstring(game:HttpGet('https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/Dream%20Island%20Tycoon'))()
else
    wait(3)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/thedragonslayer2/hey/main/Games/No%20Game%20Found"))()
end
